BITRATE	= 1500				# Default value of bitrate (used if no correct defined in any matched template)
ENFORCE_BITRATE  = -1500		# If defined and valid, that means "I surely know which video bitrate I'd like to have"
ENFORCE_TEMPLATE = # __dga|sony:720	# If defined, that means "I know which exactly templates should be used"

AVS_OVERWRITE = 0               # Should .AVS be overwrited
INDEX_ONLY  = 0			# Should only AVS+index job be created
EXTRA_AVS   = 			# if not empty, then add to .AVS file correspondend [EXTRA_AVS=xxx] section
SUFFIX 	    = 			# suffix for template (if defined will try to use 'name.suffix' template first; '.old' )
RECURSIVE   = 0			# is recursive scan for source files


[EXTRA_AVS=1]



[DETECT]
# Format: 
#	name => CONTAINTER|"VIDEO"|WIDTHxHEIGHT@FPS|VIDEO_BRATE|RATIO|VCODEC|VPROFILE|VSCAN_TYPE|VSCAN_ORDER|"AUDIO"|A_CHANNEL|A_CODEC|A_CODEC2|A_BRATE|A_ALIGNMENT|A_DELAY_MS
# 	? - replace unimportant parameter
#
# * name/value_string are trimmed; tab and spaces are allowed; 
# * Could assign several patterns to same name. Same video could match to several pattern
# * if name starts from __ - special case

#BDAV|1920x1080@50.000|25.2 Mbps|1.778|AVC|High@L4.2|Progressive||2|AC3|AC3|256 Kbps||1020.000
#MPEG-4|1440x1080@25.000|12.0 Mbps|1.778|AVC|Main@L4.0|Progressive||2|AAC LC|AAC LC|128 Kbps||

sony:720   => ?|VIDEO|1440x1080@25.000|12.0 Mbps|1.778|AVC|Main@L4.0|Progressive||AUDIO|?|?|?|?|?|?

__dga      => BDAV|VIDEO|?x?@?|?|?|?|Progressive|?||AUDIO|?|?|?|?|?|?
__forbid   => ?|VIDEO|?x?@?|?|?|?|?|Interlaced|?|AUDIO|?|?|?|?|?|?	# DO NOT ALLOW INTERLACED VIDEO


[ENCODE]
# Format:
#	name => BITRATE|AVS_TEMPLATE|INDEX_JOB|VIDEO_1PASS|VIDEO_2PASS|AUDIO_ENCODE|MUX_JOB
# 		* - means get value from upper section [ sony:720 -> sony -> '' -> '?' ]
# 		? - means unknown (should come from other section or if no such one from '?' )
# 		{} after name means adjusting
#		if bitrate <0 - use

?		=>
		=> -1000|base.avs|ffindex.idx|x264base|aac_base|mux
3gp		=> |base.avs|ffindex.idx|x264base|aac_base{Bitrate=32}|mux	# zopa 3000, 5000
canon:480	=> -1000|	#1000
canon:720	=> -3000|	#2000-3000
sony:480	=> -1000|base.avs|ffindex.idx|x264base||mux
fuji:480	=> 1000|base.avs|ffindex.idx|x264base|mp3_abr{AbrBitrate=32}|mux
sony:720	=> -5000|base.avs{Spline64Resize}|ffindex.idx|x264base||mux					# 1000{zouk},3000,5000
sony:720.new	=> 5000|base.avs|ffindex.idx|x264base{CustomEncoderOptions=--sar 4:3}||mux{DAR=@AR@}		# MP4. !!TOTEST:Require more Bitrate?
sony:720.xvid	=> -2|base.avs|ffindex.idx|xvid_cq{1pass}||
sonymts	 	=> 10010|dga.avs|dga.idx|x264avchd|aac_dga|mux							# 5000,8000,12000  !!!TOTEST!!! Looks like audio is not in sync
sonymts.old	=> -8000|dga.avs|dga.idx|x264base{KeyframeInterval=50,NbBframes=6,BframeBias=50}|aac_dga|mux 	# Old style
